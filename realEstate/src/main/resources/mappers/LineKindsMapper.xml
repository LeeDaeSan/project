<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ds.estate.model.mapper.LineKindsMapper">

	<!-- 라인 유형 정보 목록 조회 -->
	<select id="list" resultType="lineKinds">
		SELECT
			LineKindsIdx,
			LineKindsName,
			LineKindsCode,
			CreateDate
			
		  FROM LineKinds		
	
		 ORDER BY CreateDate DESC
	</select>
</mapper>