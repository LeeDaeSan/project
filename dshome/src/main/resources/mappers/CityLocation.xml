<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ds.home.realestate.mapper.CityLocationMapper">

	<select id="list" resultType="cityLocation" parameterType="cityLocation">
	
		SELECT * FROM CityLocation
		<where>
			<if test="depth != null and !depth.equals('')">
				AND Depth = #{depth}
			</if>
			
			<if test="cortarNo != null and !cortarNo.equals('')">
				AND ParentCortarNo = #{cortarNo}
			</if>
			
		</where>
	
	</select>

</mapper>