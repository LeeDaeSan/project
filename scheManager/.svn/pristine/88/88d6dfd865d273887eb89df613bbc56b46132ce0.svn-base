<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dm.sche.mapper.BoardMappingMapper">


	<!-- 등록 -->
	<insert id="insert" parameterType="boardMapping">
	
		INSERT INTO BoardMapping 
		(
			BoardManagerIdx,
			SubMenuIdx		
		) 
		VALUES 
		(
			#{boardManagerIdx},
			#{subMenuIdx}	
		)
		
	</insert>
	
</mapper>