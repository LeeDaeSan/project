/* Position */
.position_ab { position: absolute; }
.position_re { position: relative; }
.position_fi { position: fixed; }

.left0   { left: 0; }
.left100 { left: 100px; }
.left200 { left: 200px; }
.left300 { left: 300px; }
.left400 { left: 400px; }
.left500 { left: 500px; }

/* Float */
.fl { float: left; }
.fr { float: right; }
.fn { float: none; }

/* None Border */
.border0		{ border: none !important; }
.border-top0	{ border-top: none !important; }
.border-left0	{ border-left: none !important; }
.border-right0  { border-right: none !important; }
.border-bottom0 { border-bottom: none !important; }

/* Layer */
.layer_disabled { background: rgb(112, 112, 112); opacity: 0.5; }
.disabled_info { right: 387px; top: 300px; z-index: 2000; letter-spacing: 2px; }
.question-info { background-color: #999; border-radius: 5px; display: none; }
.emptyList { text-align: center; vertical-align: middle; height: 200px; font-size: 15px; }

/* Table */
.table>thead:first-child>tr:first-child>th { padding-bottom: 15px; font-weight: bold; text-align: center; }
.table>thead>tr>th { text-align: center; }
.table>tbody>tr>td { padding : 7px 11px; }

.search-table>tbody>tr>td { padding: 7px 0px 7px 10px; line-height: 28px; }

/* Modal */
.modal .table th { font-weight: normal; line-height: 34px; }
.modal .table>tbody>tr>th { vertical-align: middle; }

.custom-box { box-shadow: 2px 2px 2px #eee; border-radius: 5px; padding: 0px 10px 10px 10px; }

.form-inline label { font-weight: 700; }
.panel-header { background-color: #4dccc4; padding-top: 4px; color: #fff; text-indent: 10px; font-weight: bold; font-size: 15px; border: 1px solid #4dccc4;height:30px; }
.pagination>li>a, .pagination>li>span { color : #337ab7 }

/* icon */
.fa { display: initial !important; }

/* bootstrap customize */
.form-control{height: 28px !important; padding: 3px 5px !important; font-size: 12px !important;}
.input-group-addon{padding: 4px 12px !important;}
.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th { vertical-align: middle !important; }
#searchDateType{width:80px !important;margin-right:5px;}
.input-group-date{width:200px !important;}
