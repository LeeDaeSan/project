@charset "UTF-8";

.menuUl { height: 450px; padding-top: 10px; }

.menuLi { margin: 10px 0 10px 0; }
.menuLi .mainMenuName { color: #337ab7; font-weight: bold; cursor: pointer; }

.subMenuUl li { height: 40px; line-height: 40px; }
#sidebar > ul > li > ul.sub { display: block !important; }


.menu_disabled { top: 30px; border-radius: 5px; }
.menu_disabled_info { top: 41%; }

.height34 { height: 34px; line-height: 34px; }
.formWrap { margin: 10px 0 10px 0; }
.btn-wrap { border-top: 1px solid #eee; padding-top: 10px; }

.scrollbar
{
	overflow-y: scroll;
}

.force-overflow
{
	min-height: 450px;
	
}
/*
 *  STYLE 4
 */

#style-3::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #F5F5F5;
}

#style-3::-webkit-scrollbar
{
	width: 10px;
	background-color: #F5F5F5;
}

#style-3::-webkit-scrollbar-thumb
{
	background-color: #000000;
	border: 2px solid #555555;
}


.formWrap label { text-indent: 10px; }
.formWrap div.form-group { padding: 0px 10px 15px 10px; }